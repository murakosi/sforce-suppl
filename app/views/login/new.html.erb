<h1>Login#new</h1>
<p>Find me in app/views/login/new.html.erb</p>
<div align="center">
    <%= form_for :session, url: login_path do |f| %>
    <%= f.text_field :name %><br>
    <%= f.password_field :password %><br>
    <%= f.submit %>
    <% end %>
    <div id="message"><%= @error_message %></div>
</div>
